public class SuperButton extends Button {

    Rectangle shape;
    Skin skin;

    public SuperButton(Rectangle shape) {
        this.shape = shape;
        setWidth(shape.width);
        setHeight(shape.height);
        setBounds(shape.x, shape.y, shape.width, shape.height);
